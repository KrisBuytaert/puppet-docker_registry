
# This flavor stores the images in Aliyun OSS
# See:
# https://i.aliyun.com/access_key/
# for details on access and secret keys.
<%= scope.function_template(['docker_registry/storage/__header.erb']) %>
    storage_path: _env:STORAGE_PATH<% if defined?@storage_path then -%>:<%= @storage_path %><% end %>
    oss_host: _env:OSS_HOST<% if defined?@host then -%>:<%= @host %><% end %>
    oss_bucket: _env:OSS_BUCKET<% if defined?@bucket then -%>:<%= @bucket %><% end %>
    oss_accessid: _env:OSS_KEY<% if defined?@accessid then -%>:<%= @accessid %><% end %>
    oss_accesskey: _env:OSS_SECRET<% if defined?@accesskey then -%>:<%= @accesskey %><% end %>
